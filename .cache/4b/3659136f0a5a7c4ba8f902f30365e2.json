Sidebar = () => {\r\n    const { sidebarStyle } = this.state;\r\n    return sidebarStyle.display === 'none'\r\n      ? this.setState({\r\n          sidebarStyle: {\r\n            display: 'block'\r\n          }\r\n        })\r\n      : this.setState({\r\n          sidebarStyle: {\r\n            display: 'none'\r\n          }\r\n        });\r\n  };\r\n\r\n  render = () => {\r\n    const { sidebarStyle } = this.state;\r\n    return (\r\n      <BrowserRouter>\r\n        <FullPage>\r\n          <Navbar toggleSidebar={this.toggleSidebar} />\r\n          <MainContent>\r\n            <Sidebar sideBarStyle={sidebarStyle} />\r\n            <Switch>\r\n              <Route exact path=\"/\" component={Homescreen} />\r\n              <Route exact path=\"/:documents\" component={Docslist} />\r\n              <Route path=\"/:documents/:docId\" component={DocView} />\r\n            </Switch>\r\n          </MainContent>\r\n        </FullPage>\r\n      </BrowserRouter>\r\n    );\r\n  };\r\n}\r\n\r\nexport default App;\r\n"},"lineCount":null}},"error":null,"hash":"ef43142d68875bcec64de0336b62aa0d","cacheData":{"env":{}}}